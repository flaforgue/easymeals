generator client {
  provider = "prisma-client-js"
}

datasource postgres {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(uuid())
  idpId String @unique
}

model Recipe {
  id       String  @id @default(uuid())
  name     String
  imageUrl String?
}
